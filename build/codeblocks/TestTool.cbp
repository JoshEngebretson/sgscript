<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="TestTool" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="..\..\bin\sgstest" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\..\SGScript" />
				<Option object_output="..\..\obj\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="libsgscript.a" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="..\..\bin\sgstest" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\..\SGScript" />
				<Option object_output="..\..\obj\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-fomit-frame-pointer" />
					<Add option="-fexpensive-optimizations" />
					<Add option="-O3" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="libsgscript.a" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-D_DEBUG=1" />
			<Add directory="..\..\src" />
		</Compiler>
		<Linker>
			<Add library="libsgscript.a" />
			<Add directory="..\..\lib" />
		</Linker>
		<Unit filename="..\..\ext\sgstest.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
